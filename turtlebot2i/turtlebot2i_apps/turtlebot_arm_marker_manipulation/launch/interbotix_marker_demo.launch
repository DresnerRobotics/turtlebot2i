<launch>
  <arg name="auto_draw" default="false"/>
  <arg name="at_dock" default="false"/>

  <include file="$(find turtlebot_arm_marker_manipulation)/launch/marker_manipulation.launch">
    <arg name="at_dock" value="$(arg at_dock)"/>
  </include>

  <node name="marker_manipulation_demo" pkg="turtlebot_arm_marker_manipulation" type="marker_manipulation_demo" output="screen" >
    <param name="arm_link" value="/arm_base_link" />
    <param name="gripper_open" value="0.031" />

    <param name="block_size" value="0.02" /> <!--TODO: Change to toolholder size-->
    <!--param name="tool_size" value="0.0185"/-->
    <!--param name="tool_height" value="0.118"/-->
    <param name="gripper_tighten" value="0.006" />
    <param name="table_height" value="0.0" /> <!--Relative to base_link-->

    <param name="auto_draw" value="$(arg auto_draw)" />

  </node>
</launch>